{
  "manifest_version": 2,
  "name": "CzateriaMod",
  "version": "0.1",
  "description": "CzateriaMod by Stephanov",
  "author": "Stephanov",
  
  "background": {
      "persistent": true,
      "scripts": ["js/background.js"]
    },
  "permissions": [
       "contextMenus",
       "activeTab"
     ],
	  
	 
	"web_accessible_resources": [
		"note.mp3"
	],
	
  "icons": {
       "16": "icons/icon16x16.png",
       "48": "icons/icon48x48.png",
       "128": "icons/icon128x128.png"
       }
}